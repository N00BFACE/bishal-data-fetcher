
{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "bdf/bdf-block",
    "version": "0.1.0",
    "title": "BDF Block",
    "description": "A block to display data from the BDF API.",
    "category": "widgets",
    "icon": "download",
    "keywords": ["bishal", "data", "fetcher", "api", "block"],
    "attributes": {
        "showId": {
            "type": "boolean",
            "default": true
        },
        "showFirstName": {
            "type": "boolean",
            "default": true
        },
        "showLastName": {
            "type": "boolean",
            "default": true
        },
        "showEmail": {
            "type": "boolean",
            "default": true
        },
        "showDate": {
            "type": "boolean",
            "default": true
        }
    },
    "supports": {
        "html": false
    },
    "textdomain": "bishal-data-fetcher",
    "editorScript": "file:./index.js",
    "render": "file:./render.php"
}